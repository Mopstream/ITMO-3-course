-- 1 --
select Н_ЛЮДИ.ИМЯ, Н_ВЕДОМОСТИ.ЧЛВК_ИД from Н_ЛЮДИ
left join Н_ВЕДОМОСТИ on Н_ВЕДОМОСТИ.ЧЛВК_ИД = Н_ЛЮДИ.ИД
where Н_ЛЮДИ.ИМЯ = 'Александр' and
      Н_ВЕДОМОСТИ.ДАТА > '1998-01-05' and
      Н_ВЕДОМОСТИ.ДАТА = '2022-06-08';

-- 2 --

select Н_ЛЮДИ.ФАМИЛИЯ, Н_ОБУЧЕНИЯ.НЗК, Н_УЧЕНИКИ.НАЧАЛО from Н_ЛЮДИ
join Н_ОБУЧЕНИЯ on Н_ОБУЧЕНИЯ.ЧЛВК_ИД = Н_ЛЮДИ.ИД
join Н_УЧЕНИКИ on Н_УЧЕНИКИ.ЧЛВК_ИД = Н_ОБУЧЕНИЯ.ЧЛВК_ИД
where 
    Н_ЛЮДИ.ИД > 152862 and
    Н_ОБУЧЕНИЯ.НЗК = '933232';
    
